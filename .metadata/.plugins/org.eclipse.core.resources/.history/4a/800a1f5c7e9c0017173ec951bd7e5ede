package br.com.indra.jovemprofissional.model;

import javax.persistence.Entity;
import javax.persistence.Id;

@Entity(name = "team")
public class Team{

	@Id	
	private int id;

	private String nome;

	
	public Team(){}

	public Team(int _ID, String _NOME){
		this.ID = _ID;
		this.NOME = _NOME;

	}
	
	public int getID() {
		return ID;
	}
	
	public String getNOME() {
		return NOME;
	}
	
	public void setID(int iD) {
		ID = iD;
	}
	
	public void setNOME(String nOME) {
		NOME = nOME;
	}

}