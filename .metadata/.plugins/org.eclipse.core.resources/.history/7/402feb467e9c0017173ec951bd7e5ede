package br.com.indra.jovemprofissional.model;

import javax.persistence.Entity;
import javax.persistence.Id;

@Entity(name = "TB_TEAM")
public class Team{

	@Id	
	private int ID;

	private String NOME;

	
	public Team(){}

	public Team(int _ID, String _NOME){
		this.ID = _ID;
		this.NOME = _NOME;

	}
	
	public int getID() {
		return ID;
	}
	
	public String getNOME() {
		return NOME;
	}

}